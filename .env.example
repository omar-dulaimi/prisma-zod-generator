# Multi-Provider Test Environment Configuration
# Copy this file to .env and configure your database connections

# PostgreSQL
POSTGRESQL_URL="postgresql://user:password@localhost:5432/test"

# MySQL
MYSQL_URL="mysql://user:password@localhost:3306/test"

# MongoDB
MONGODB_URL="mongodb://localhost:27017/test"

# SQLite (file-based, no configuration needed)
SQLITE_URL="file:./test.db"

# SQL Server
SQLSERVER_URL="sqlserver://localhost:1433;database=test;user=sa;password=password;encrypt=true;trustServerCertificate=true"

# Test Configuration
TEST_TIMEOUT=30000
TEST_PARALLEL=false
TEST_VERBOSE=false