{
  "mode": "custom",
  "output": "./prisma/generated",
  "models": {
    "User": {
      "enabled": true,
      "operations": ["findMany", "findUnique", "create", "update"],
      "variants": {
        "input": { "excludeFields": ["role", "permissions"] },
        "result": { "excludeFields": [] },
        "pure": { "excludeFields": [] }
      }
    },
    "Account": {
      "enabled": false
    }
  }
}
